<channel
   name="Random ADT Message Generator"
   guid="BE925FA7C87A65E7CA3A58AE1E07FA9D"
   description="This channel generates a random HL7 ADT message every 10 seconds and then sends it via LLP to localhost:5145.&#xD;&#xA;&#xD;&#xA;Once imported, you can change the IP address, port and content of the generated messages to suit your specific needs."
   start_automatically="false"
   logging_level="1"
   use_message_filter="false">
   <to_llp_client
      remote_host="localhost"
      remote_port="5145"
      wait_for_ack="true"
      ack_timeout="10000"
      ack_verification_type="0"
      ack_vmd_path="${iguana_dir}/ack_verify.vmd"
      retry_on_ack_timeout="true"
      retry_on_ack_verification_failed="false"
      unlimited_count_of_retry="false"
      count_of_retry="5"
      disconnect_between_retries="true"
      ack_reconnect_interval="10000"
      ack_error_handling="1"
      persistent_connection="true"
      connection_timeout="10000"
      unlimited_max_count_of_reconnect="false"
      max_count_of_reconnect="60"
      connection_lost_reconnect_interval="10000">
      <dequeue_list>
         <dequeue
            source_guid="00000000000000000000000000000000"
            dequeue_guid="31A3AE75F570BA3AB4AE0147B54C9671"/>
      </dequeue_list>
   </to_llp_client>
   <from_mapper
      guid="Random_ADT_Message_Generator-From-6lvpNA6tBBa0X5"
      commit_id="2b46bb31874c6af7d3957eacc5f61afb00a530aa"
      commit_comment="Random ADT messages"
      poll_time="10000">
   </from_mapper>
</channel>
